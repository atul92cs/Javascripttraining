<div class="main-body">
    <div class="data-section">
        {{#each car}}
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">{{this.Name}}</h5>
                <p class="card-text">{{this.Model}}</p>
                <p class="card-text"><small class="text-muted">{{this.Company}}</small></p>
            </div>
        </div>
        {{/each}}
    </div>
</div>